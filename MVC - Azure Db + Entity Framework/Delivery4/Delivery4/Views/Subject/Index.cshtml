@model IEnumerable<Delivery4.Subject>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDSubject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PersonIDPerson)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IDSubject)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PersonIDPerson)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IDSubject }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IDSubject })
            </td>
        </tr>
    }

</table>
@Html.ActionLink("People","Index", "Person")

